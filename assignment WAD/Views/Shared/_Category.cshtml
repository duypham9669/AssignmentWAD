@model IEnumerable<assignment_WAD.Models.category>
<div class="nn-block">
    <div class="nn-megamenu-sidebar-title"><i class="glyphicon glyphicon-list"></i><h2>categories</h2></div>
    @foreach (var c in Model)
    {
        <div class="nn-megamenu-sidebar">
            <ul>
                <li>
                    <a href="/Product/Category?CategoryID=@c.categoryId">
                        <h2 class="col-md-9"><strong>@c.categoryName</strong></h2>
                    </a>
                </li>
            </ul>
        </div>
    }
</div><!--end block category-->